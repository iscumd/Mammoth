<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>mammoth_snowplow</name>
  <version>0.0.1</version>
  <description>Mammoth is a friendly snow plowing robot. This package is for the Mammoth robot.</description>
  <maintainer email="isc-robotics@umich.edu">Intelligent Systems Club</maintainer>
  <maintainer email="dcutting@umich.edu">David Cutting</maintainer>
  <maintainer email="agirven@umich.edu">Avery Girven</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <exec_depend>mammoth_description</exec_depend>
  <exec_depend>ros2_ouster</exec_depend> 
  <exec_depend>lidar_processor</exec_depend>
  <exec_depend>isc_roboteq</exec_depend>        
  <exec_depend>realsense2_camera</exec_depend>
  <depend>robot_state_controller</depend>
  <test_depend>ament_lint_auto</test_depend>

  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
