    </collision>
    <inertial>
      <mass value="1.1"/>
      <inertia
        ixx="0.0048125" ixy="0.0"       ixz="0.0"
                        iyy="0.0048125" iyz="0.0"
                                        izz="0.00859375"/>
    </inertial>
  </link>

  <joint name="base_to_wheel_al" type="continuous">
    <axis xyz="0 0 1"/>
    <origin rpy="1.57 -3.14159 0" xyz="0.15 -0.2375 -0.1"/>
